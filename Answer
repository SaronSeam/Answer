public static bool is_valid_IP(string IpAddres)
{
    string[] adress = IpAddres.Split('.');

    if (adress.Length!=4)
    {
        return false;
    }

    for (int i = 0; i < 4; i++)
    {
        int result;
        if (adress[i][0]=='0')
        {
            return false;
        }
        if (!int.TryParse(adress[i], out result))
        {
            return false;
        }
        if (result >255)
        {
            return false;
        }
    }


    return true;
}
